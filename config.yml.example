bot:
  token: null
  timeout: 10
forwarding:
  moderation_chat_id: null
  publication_chat_id: null
  message_template: "<b>Сообщение из предложки</b>\n\n<blockquote>{text}</blockquote>"
  types:
  - text
  - photo
  - video
openai:
  api_key: null
  timeout: 10
  max_retries: 0
moderation:
  enabled: true
  model: gpt-5-mini
  types:
  - links
  - omni
  - gpt
logging:
  level: INFO
  fmt: '%(asctime)s.%(msecs)03d %(levelname)s [%(name)s] %(message)s'
  date_fmt: '%Y-%m-%d %H:%M:%S'
